  "metadata": {
    "purpose": "The BizBanking Client Onboarding process establishes new banking relationships with business clients (annual turnover up to â‚¬10M) by collecting, validating, and processing client information to create fully operational accounts. This includes identity verification through KYC checks, regulatory compliance screening (AML, PEP, Sanctions), risk assessment, and where requested, credit facility assessment for overdraft products. This process is critical for revenue generation and regulatory compliance.",
    "trigger": "Process triggered via four channels: (1) online submission via Business Portal, (2) RM paper application, (3) branch network referral, (4) partner channel submission. Formally initiates when application enters CRM queue with minimum required documentation.",
    "frequency": "Continuous during business hours, real-time processing. Peak periods at month-end, quarter-end, January, and September.",
    "volume": "80-120 applications/month (60% of total bank volume). 70% Standard, 30% Complex. 12-15% YoY growth. 4-6 hours average processing time.",
    "confidence": "HIGH"
  },

  "process_steps": [
    {"id": "PS-COB-001", "sequence": 1, "name": "Application Receipt & Initial Triage", "owner": "Operations Officer", "systems": ["CRM", "DMS", "OWS"], "rationale": "Ensure complete applications enter pipeline, reject incomplete early", "sla": "Same business day", "status": "VALIDATED"},
    {"id": "PS-COB-002", "sequence": 2, "name": "KYC & Identity Verification", "owner": "KYC Analyst", "systems": ["Fenergo", "World-Check", "CRO Portal", "Jumio"], "rationale": "Regulatory compliance (CDD, AML), risk mitigation", "sla": "2-5 business days", "status": "VALIDATED"},
    {"id": "PS-COB-003", "sequence": 3, "name": "Credit Assessment", "owner": "Credit Analyst", "systems": ["CDS", "Bureau Link"], "rationale": "Credit risk management, responsible lending", "sla": "3 business days", "condition": "If overdraft requested", "status": "VALIDATED"},
    {"id": "PS-COB-004", "sequence": 4, "name": "Account Setup & Configuration", "owner": "Operations Officer", "systems": ["CBS (T24)", "Marqeta"], "rationale": "Enable client to transact, complete product setup", "sla": "1 business day", "status": "VALIDATED"},
    {"id": "PS-COB-005", "sequence": 5, "name": "Client Communication & Activation", "owner": "Relationship Manager", "systems": ["CRM", "Email", "CBS"], "rationale": "Client engagement, ensure smooth activation", "sla": "1 business day", "status": "VALIDATED"},
    {"id": "PS-COB-006", "sequence": 6, "name": "Post-Onboarding Quality Check", "owner": "Team Lead", "systems": ["OWS", "QMS"], "rationale": "Quality assurance, continuous improvement", "sla": "Within 10 business days", "status": "VALIDATED"}
  ],
